{
  "name": "@agoralabs-sh/agora-labs-website",
  "version": "1.2.0",
  "description": "The front facing website for the Agora Labs.",
  "repository": {
    "type": "git",
    "url": "https://github.com/agoralabs-sh/agora-labs-website"
  },
